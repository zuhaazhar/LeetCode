int singleNumber(int* nums, int numsSize) {
    int k = 0, l = 0, m = 0;
    for (int i = 0; i < numsSize; i++) {
        k = 0;
        for (int j = 0; j < numsSize; j++) {
            if (nums[j] == nums[i]) {
                k++;
            }
        }
        if (k != 2)
            m = nums[i];
    }
    return m;
}
